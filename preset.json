{
  "useConfigFiles": true,  
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-pwa": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": ["save"]
    },
    "vue-cli-plugin-vuetify": {
      "preset": "configure",
      "replaceComponents": false,
      "useTheme": false,
      "useCustomProperties": false,
      "iconFont": "mdi",
      "installFonts": false,
      "useAlaCarte": true,
      "usePolyfill": true,
      "locale": "en"
    }
  },
  "router": true,
  "vuex": true,
  "cssPreprocessor": "dart-sass"
}